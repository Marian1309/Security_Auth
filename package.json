{
  "name": "nextjs14-template",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Marian",
    "email": "pidchashymaryan@gmail.com",
    "url": "https://github.com/Marian1309"
  },
  "scripts": {
    "check:updates": "ncu -i",
    "depcheck": "depcheck",
    "clean": "rimraf bun.lockb && rimraf .next && rm -rf node_modules",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "dev": "next dev",
    "build": "next build",
    "prod": "next build && next start",
    "prod:bundlecheck": "cross-env ANALYZE=true next build && next start",
    "format": "prettier . -w",
    "lint": "eslint --fix",
    "lint-staged": "lint-staged",
    "shadcn": "bunx --bun shadcn@latest add",
    "shadcn:all": "bunx --bun shadcn@latest add --all",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev --name",
    "db:gen": "prisma generate",
    "db:studio": "prisma studio"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "pretty-quick"
    ]
  },
  "browserslist": {
    "production": [
      "> 0.1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tanstack/react-query": "^5.59.13",
    "@tanstack/react-query-devtools": "^5.59.20",
    "framer-motion": "^11.11.11",
    "lucide-react": "^0.452.0",
    "next": "14.2.15",
    "next-themes": "^0.4.3",
    "node-mailjet": "^6.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-spinners": "^0.14.1",
    "sharp": "^0.33.5",
    "zod": "^3.23.8",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/format": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@next/bundle-analyzer": "^14.2.15",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20",
    "@types/nodemailer-smtp-transport": "^2.7.8",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "commitlint": "^19.5.0",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-check-file": "^2.8.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "fluid-tailwind": "^1.0.3",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "pretty-quick": "^4.0.0",
    "prisma": "^5.21.1",
    "sass": "^1.79.5",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
  },
  "packageManager": "bun@1.1.30",
  "engines": {
    "node": ">=20.x.x"
  }
}
